{
  "name": "pptr-visual",
  "version": "1.0.0",
  "description": "Visual Regressin Testing with Puppeteer",
  "main": "index.js",
  "scripts": {
    "test-snapshots": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "test-snapshots-update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles --forceExit",
    "test-percy": "percy exec -- jest ./tests-snapshots/__tests__/percy.test.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jest": "^28.1.3",
    "jest-image-snapshot": "^5.2.0",
    "percy": "^5.0.0",
    "prettier": "^2.7.1",
    "puppeteer": "^17.1.1"
  },
  "devDependencies": {
    "@percy/cli": "^1.10.2",
    "@percy/puppeteer": "^2.0.2"
  }
}
